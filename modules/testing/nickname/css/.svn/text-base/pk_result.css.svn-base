.progress {
  width: 80%;
  height: 0.10667rem;
  border-radius: 0.04267rem;
  /*margin: r(30) auto;*/
  background: #c2c2c2;
  position: relative; }
  .progress .progress-bar {
    background: #fbbd06;
    height: 100%;
    border-radius: 0.04267rem;
    -webkit-transition: 0.4s linear;
    -moz-transition: 0.4s linear;
    -o-transition: 0.4s linear;
    transition: 0.4s linear;
    -webkit-transition-property: width;
    -moz-transition-property: width;
    -o-transition-property: width;
    transition-property: width; }

.block {
  display: table;
  position: relative;
  padding-top: 0.128rem;
  width: 100%;
  height: 0.85333rem; }
  .block .left {
    position: relative;
    display: table-cell;
    width: 50%;
    padding-right: 0.21333rem;
    box-sizing: content-box; }
    .block .left .count {
      padding-left: 0.17067rem; }
    .block .left .progress {
      position: absolute;
      right: 0.21333rem; }
      .block .left .progress .progress-bar {
        position: absolute;
        right: 0; }
  .block .center {
    position: absolute;
    width: 100%;
    left: 0;
    top: 0.08533rem;
    z-index: 1; }
    .block .center span {
      display: block;
      margin: 0 auto;
      text-align: center; }
    .block .center .desc {
      padding-top: 0.04267rem; }
  .block .right {
    position: relative;
    display: table-cell;
    width: 50%;
    padding-left: 0.21333rem;
    box-sizing: content-box; }
    .block .right .count {
      text-align: right;
      padding-right: 0.17067rem; }
    .block .right .progress {
      position: absolute;
      left: 0.21333rem; }
      .block .right .progress .progress-bar {
        position: absolute;
        left: 0; }
  .block .desc {
    display: inline-block; }

