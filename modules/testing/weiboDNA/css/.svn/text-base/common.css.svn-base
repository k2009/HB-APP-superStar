body {
  padding-bottom: 60px; }

.mui-title {
  position: static;
  color: #555;
  margin: 0; }

.btnBox {
  text-align: center;
  display: flex;
  display: -webkit-flex;
  justify-content: space-around;
  flex-direction: row;
  -webkit-flex-direction: row; }

.mui-btn {
  background: #fbbd06;
  border-radius: 5px;
  text-align: center;
  color: white;
  -webkit-flex: 1;
  flex: 1;
  margin: auto 10px; }
