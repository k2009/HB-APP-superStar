.markform {
  width: 100%;
  background: white;
  position: relative;
  height: 200px;
  overflow: hidden; }

.markOuter {
  border: 2px solid #ecd7b4;
  border-top: none;
  border-right: none;
  width: 80%;
  height: 120px;
  margin: 15px auto;
  margin-left: 45px;
  position: relative;
  background: white; }

.markYPicker {
  position: absolute;
  left: -35px;
  width: 35px;
  display: inline-block;
  font-size: 12px;
  text-align: left; }

.markYPicker:after {
  content: '';
  position: absolute;
  right: 0;
  top: 10px;
  display: inline-block;
  width: 8px;
  border-bottom: 1px solid #c2b8a4; }

.markXPicker {
  position: absolute;
  bottom: -40px;
  display: inline-block;
  font-size: 12px;
  transform: rotate(45deg);
  -webkit-transform: rotate(45deg);
  white-space: nowrap; }

.markXPicker:after {
  content: '';
  position: absolute;
  top: 10px;
  left: -9px;
  display: inline-block;
  height: 6px;
  border-left: 1px solid #c2b8a4;
  transform: rotate(-45deg);
  -webkit-transform: rotate(-45deg); }

.marker {
  position: absolute;
  width: 10px;
  height: 10px;
  border-radius: 10px;
  background: #f9784b;
  display: inline-block; }

.marker:after {
  content: '';
  position: absolute;
  width: 6px;
  height: 6px;
  border-radius: 5px;
  background: white;
  /* 	background-image: -webkit-linear-gradient(to bottom, #d9e8c5, #93c364); Chrome 10-25, iOS 5+, Safari 5.1+
  	background-image:         linear-gradient(to bottom, #d9e8c5, #93c364); Chrome 26, Firefox 16+, IE 10+, Opera */
  top: 2px;
  left: 2px; }

.markerTip {
  position: absolute;
  display: inline-block;
  width: 36px;
  height: 22px;
  text-align: center;
  line-height: 20px;
  color: #555;
  background: url(../images/icon.png) no-repeat;
  font-size: 12px;
  top: -24px;
  left: -13px; }
